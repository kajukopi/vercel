<!DOCTYPE html>
<html lang="en">

<head>
  <!-- COMMON TAGS -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Form Undangan Digital</title>
  <!-- Search Engine -->
  <meta name="description" content="Form Undangan Digital by Karim Roy Tampubolon">
  <meta name="image" content="/android-chrome-512x512.png">
  <!-- Schema.org for Google -->
  <meta itemprop="name" content="Form Undangan Digital">
  <meta itemprop="description" content="Form Undangan Digital by Karim Roy Tampubolon">
  <meta itemprop="image" content="/android-chrome-512x512.png">
  <!-- Open Graph general (Facebook, Pinterest & LinkedIn) -->
  <meta property="og:title" content="Form Undangan Digital">
  <meta property="og:description" content="Form Undangan Digital by Karim Roy Tampubolon">
  <meta property="og:image" content="/android-chrome-512x512.png">
  <meta property="og:url" content="https://undangan-digital.xyz/">
  <meta property="og:site_name" content="Form Undangan Digital">
  <meta property="og:locale" content="en_US">
  <meta property="og:type" content="website">
  <!-- Twitter -->
  <meta property="twitter:card" content="summary">
  <meta property="twitter:title" content="Form Undangan Digital">
  <meta property="twitter:description" content="Form Undangan Digital by Karim Roy Tampubolon">
  <meta property="twitter:image:src" content="/android-chrome-512x512.png">
  <!-- Favicon -->
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link rel="manifest" href="/site.webmanifest">
  <link rel="stylesheet" href="css/all.css">
  <link rel="stylesheet" href="css/core.css">
  <script src="https://accounts.google.com/gsi/client" async defer></script>
  <style>
    @media (min-width: 1000px) {

      main,
      header,
      #main-navbar {
        padding-left: 240px;
      }
    }
  </style>
</head>

<body>
  <!--Main Navigation-->
  <header>
    <!-- Sidenav -->
    <nav id="sidenav" class="sidenav" data-mdb-hidden="false" data-mdb-accordion="true">
      <a class="ripple d-flex justify-content-center py-4" href="#!" data-mdb-ripple-color="primary">
        <img id="MDB-logo" src="favicon-32x32.png" alt="MDB Logo" draggable="false" />
      </a>

      <ul class="sidenav-menu">
        <li class="sidenav-item">
          <a class="sidenav-link" href="/">
            <i class="fa-solid fa-home fa-fw me-3"></i><span>Home</span></a>
        </li>
        <li class="sidenav-item">
          <a class="sidenav-link"><i class="fa-solid fa-table fa-fw me-3"></i><span>Form</span></a>
          <ul class="sidenav-collapse">
            <li class="sidenav-item">
              <a href="/pernikahan" class="sidenav-link">Pernikahan</a>
            </li>
            <li class="sidenav-item">
              <a class="sidenav-link">Khitanan</a>
            </li>
            <li class="sidenav-item">
              <a class="sidenav-link">Ulang Tahun</a>
            </li>
            <li class="sidenav-item">
              <a class="sidenav-link">Lain - lain</a>
            </li>
          </ul>
        </li>
        <li class="sidenav-item">
          <a class="sidenav-link"><i class="fa-solid fa-users fa-fw me-3"></i><span>Tamu Undangan</span></a>
          <ul class="sidenav-collapse">
            <li class="sidenav-item">
              <a class="sidenav-link">List</a>
            </li>
            <li class="sidenav-item">
              <a class="sidenav-link">Log</a>
            </li>
          </ul>
        </li>
      </ul>
    </nav>
    <!-- Sidenav -->

    <!-- Navbar -->
    <nav id="main-navbar" class="navbar navbar-expand-lg navbar-light bg-white fixed-top">
      <!-- Container wrapper -->
      <div class="container-fluid">
        <!-- Toggler -->
        <button id="button-menu" data-mdb-toggle="sidenav" data-mdb-target="#sidenav"
          class="btn shadow-0 p-0 me-3 d-block d-lg-none" aria-controls="#sidenav" aria-haspopup="true">
          <i class="fas fa-bars fa-lg"></i>
        </button>

        <!-- Search form -->
        <form class="d-none d-md-flex input-group w-auto my-auto">
          <input autocomplete="off" type="search" class="form-control rounded"
            placeholder='Search (ctrl + "/" to focus)' style="min-width: 225px" />
          <span class="input-group-text border-0"><i class="fas fa-search"></i></span>
        </form>

        <!-- Right links -->
        <ul class="navbar-nav ms-auto d-flex flex-row">
          {{#if users}}
          <!-- Notification dropdown -->
          <li class="nav-item dropdown">
            <a class="nav-link me-3 me-lg-0 dropdown-toggle hidden-arrow" href="#" id="navbarDropdownMenuLink"
              role="button" data-mdb-toggle="dropdown" aria-expanded="false">
              <i class="fas fa-bell"></i>
              <span class="badge rounded-pill badge-notification bg-danger">1</span>
            </a>
            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownMenuLink">
              <li><a class="dropdown-item" href="#">Some news</a></li>
              <li><a class="dropdown-item" href="#">Another news</a></li>
              <li>
                <a class="dropdown-item" href="#">Something else here</a>
              </li>
            </ul>
          </li>

          {{!-- <!-- Icon -->
          <li class="nav-item">
            <a class="nav-link me-3 me-lg-0" href="#">
              <i class="fas fa-fill-drip"></i>
            </a>
          </li> --}}

          <!-- Avatar -->
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle hidden-arrow d-flex align-items-center" href="#"
              id="navbarDropdownMenuLink" role="button" data-mdb-toggle="dropdown" aria-expanded="false">
              <img src="img/users/{{users.name}}.jpg" class="rounded-circle" height="22" alt="{{users.email}}"
                loading="lazy"  />
            </a>
            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownMenuLink">
              <li><a class="dropdown-item" href="#">My profile</a></li>
              <li><a class="dropdown-item" href="#">Settings</a></li>
              <li><a class="dropdown-item" href="/signout">Signout</a></li>
            </ul>
          </li>
          {{else}}
          <!-- Sign in -->
          <li class="nav-item">
            <div id="g_id_onload"
              data-client_id="1077948418495-7vdgn6ctjpb7n488hae5iai0q21mg6c1.apps.googleusercontent.com"
              data-context="use" data-ux_mode="popup" data-callback="callBackSignIn" data-auto_prompt="false">
            </div>
            <div class="g_id_signin" data-type="standard" data-shape="pill" data-theme="outline" data-text="signin_with"
              data-size="medium" data-logo_alignment="center">
            </div>
          </li>
          {{/if}}


        </ul>
      </div>
      <!-- Container wrapper -->
    </nav>
    <!-- Navbar -->
  </header>
  <!--Main Navigation-->

  <!--Main layout-->
  <main style="margin-top: 58px">
    <div class="container">
      {{{body}}}
    </div>
  </main>
  <!--Main layout-->

  <!--Footer-->
  <footer></footer>
  <!--Footer-->

</body>

<script src="js/core.js" type="text/javascript"></script>
<script src="js/module.js" type="text/javascript"></script>
<script src="js/main.js" type="text/javascript"></script>

{{#if script}}
<script src="js/pages/{{script}}.js" type="text/javascript"></script>
{{/if}}

</html>